#!/bin/bash -e
export VOLTDB_HOME=/usr/local/voltdb-3.0
export VOLTDB_HEAPSIZE=256
export CLASSPATH=/usr/local/jars/jackson-core-2.1.1.jar:/usr/local/jars/jackson-databind-2.1.1.jar:/usr/local/jars/jackson-annotations-2.1.1.jar:/usr/local/clojure/clojure-1.4.0.jar:classes
bin/custom-voltdb create catalog voltdb/pb-schema.jar
