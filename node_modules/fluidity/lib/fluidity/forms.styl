darrow = unquote("url('data:image/png;base64,')")

cal = unquote("url('data:image/png;base64,')")

// --------------------------------------------------
// Forms
// --------------------------------------------------
// GENERAL STYLES
// --------------
// Make all forms have space below them
// --------------------------------------------------
form
  grid(100%,100%)
  margin 0 0 base-line-height

fieldset
  padding (base-font-size * 2) 0 0 0 !important
  border 0
  margin-bottom 1em

legend
  display block
  width 100%
  height base-font-size * 2.4
  padding 0
  margin 0
  font-size base-font-size * 1.2
  line-height base-line-height * 2
  border 0
  border-bottom 1px solid lighter-color2

label
  line-height base-line-height
  padding-top (base-line-height / 3)
  padding-bottom (base-line-height / 3)
  padding-right (base-line-height/2)
  box-sizing border-box

// Form controls
// -------------------------
textarea,
input[type="text"],
input[type="password"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
  appearance none
  outline none
  height base-line-height
  padding 1em 0.5em
  margin-bottom base-line-height
  font-size base-font-size
  line-height base-line-height
  vertical-align middle
  box-sizing border-box

textarea
  min-height base-line-height * 6

input[type="color"]
  background-color lighten(lighter-color3,30%)

input[type="date"]
input[type="datetime"]
input[type="datetime-local"]
  appearance none
  outline none
  position relative
  background-color lighten(lighter-color3, 30%)
  background-image cal
  background-repeat no-repeat
  background-size base-line-height
  background-position 95% 45%
  padding 0.4em 0.5em
  border 1px solid lighter-color2
  border-radius 3px
  box-sizing border-box
  color darker-color2
  &::-webkit-inner-spin-button
  &::-webkit-outer-spin-button
    appearance none
  &::-webkit-calendar-picker-indicator
    display block
    position absolute
    top 0
    left 0
    width 100%
    height 100%
    border 1px solid
    background-color lighten(lighter-color3, 30%)
    opacity 0
  &:hover,
  &:focus,
  &:active
    border 1px solid lighter-color2
    box-shadow inset 0 0px 3px 0px lighten(lighter-color2,15%)
    background-color lighten(lighter-color3, 15%)
    border-radius 3px

input[type="submit"],
button
  background-color lighten(lighter-color3, 30%)
  margin base-line-height (base-line-height/2)
  padding 0.5em 0.5em
  border 1px solid lighter-color2
  box-shadow inset 0 0px 0px rgba(0,0,0,0)
  translate3d 0 0 0
  transition border linear .2s, box-shadow linear .2s
  border-radius 0px
  color darker-color2
  &:hover,
  &:focus,
  &:active
    border 1px solid lighter-color2
    box-shadow inset 0 0px 3px 0px lighten(lighter-color2,15%)
    background-color lighten(lighter-color3, 15%)
    border-radius 3px

output
  display block
  margin 0
  padding 0

select
  appearance none
  outline none
  background-color lighten(lighter-color3, 30%)
  background-image darrow
  background-repeat no-repeat
  background-size (base-line-height/ 1.4)
  background-position 95% 45%
  padding 0.5em 0.5em
  border 1px solid lighter-color2
  box-shadow inset 0 0px 0px rgba(0,0,0,0)
  translate3d 0 0 0
  transition border linear .2s, box-shadow linear .2s
  border-radius 3px
  color darker-color2
  option
    appearance none
    outline none
    padding 0
    margin-left 0
    margin-right 3em
  &:hover,
  &:focus,
  &:active
    border 1px solid lighter-color2
    box-shadow inset 0 0px 3px 0px lighten(lighter-color2,15%)
    background-color lighten(lighter-color3, 15%)
    border-radius 3px

// CHECKBOXES & RADIOS
// -------------------
input[type="radio"],
input[type="checkbox"]
  position relative
  margin (base-line-height / 2.95) 1em (base-line-height / 3) 0
  appearance none
  outline none
  padding 0.6em
  background-color lighten(lighter-color3, 30%)
  border 1px solid lighter-color2
  translate3d 0 0 0
  transition background-color linear .12s, box-shadow linear .12s, border linear .12s
  transition-delay 0s
  &:checked
    appearance none
    outline none
    background-color darker-color2
    transform scale(0.9,0.9)

input[type="radio"]
  border-radius 50%
  &:checked
    box-shadow inset 0px 0px 0px 0.2em lighten(lighter-color3, 30%)

input[type="checkbox"]
  border-radius 0px

input[type="checkbox"]:checked
  border-radius 3px
  box-shadow inset 0px 0px 0px 0.2em lighten(lighter-color3, 30%)

// Focus Transitions
textarea,
input[type="text"],
input[type="password"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
input[type="color"]
  appearance none
  border 1px solid lighter-color2
  box-shadow inset 0 0px 0px rgba(0,0,0,0)
  translate3d 0 0 0
  transition border linear .2s, box-shadow linear .2s
  border-radius 0px
  &:focus
    border-color darken(lighter-color2, 3%)
    border-radius 3px
    outline none
    box-shadow inset 0 1px 9px 1px lighter-color3

// DISABLED STATE
// --------------
input[disabled],
select[disabled],
textarea[disabled],
input[readonly],
select[readonly],
textarea[readonly]
  cursor: not-allowed;
  background-color: lighter-color1;

// Explicitly reset the colors here
input[type="radio"][disabled],
input[type="checkbox"][disabled],
input[type="radio"][readonly],
input[type="checkbox"][readonly]
  background-color: transparent;

// HTML5 invalid states
// Shares styles with the .control-group.error above
input:focus:invalid,
textarea:focus:invalid,
select:focus:invalid
  border-color #ee5f5b
  &:focus
    border-color darken(#ee5f5b, 10%)
    box-shadow 0 0 6px lighten(#ee5f5b, 20%)

// --------------------------------------------------
// Variables Needed applied to form element
// --------------------------------------------------
// form-min-width Npx
// layout horizontal,vertical,inline
// labels top,left,right
// --------------------------------------------------

form(l = horizontal,w = 100%, a = left)
  display block
  width w
  label
    display block
    width 25%
    float left
    text-align right
  input[type="text"],
  input[type="password"],
  input[type="number"],
  input[type="email"],
  input[type="url"],
  input[type="search"],
  input[type="tel"],
    display block
    width 55%
    margin-right 20%
    float left
  textarea
    display block
    width 60%
    margin-right 2%
    float left
  select
  input[type="datetime"],
  input[type="datetime-local"],
  input[type="date"],
  input[type="month"],
  input[type="time"],
  input[type="week"]
    display block
    width 55%
    margin-right 20%
    float left
  input[type="radio"],
  input[type="checkbox"]
    width auto
    float left
  input[type="radio"] + label,
  input[type="checkbox"] + label
    float left
    clear left
    width 25%
  input[type="submit"],
  button
    float right
  fieldset input[type="submit"]:first-of-type,
  fieldset button:first-of-type
    margin-right 15% !important
