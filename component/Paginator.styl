.Paginator
  a:hover
    background-color #bbb
    color #fff
    border-radius 8px / 5px
    position relative
    z-index 1
    text-shadow 0 -1px 0 #444
  a, strong
    padding 3px 7px 5px 7px
    margin 10px 20px
    font-size 1.25em
    line-height 1.25em
    letter-spacing 2px
  strong
    font-weight bold
    line-height 1.1em
    color #888
